<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:keepScreenOn="true">

  <com.google.android.exoplayer2.ui.PlayerView android:id="@+id/player_view"
      android:layout_width="match_parent"
      android:layout_height="match_parent"/>

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:background="#33000000"
      android:orientation="vertical">

    <TextView android:id="@+id/debug_text_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="4dp"
        android:paddingRight="4dp"
        android:textSize="10sp"
        tools:ignore="SmallSp"
        android:visibility="gone"/>

    <LinearLayout android:id="@+id/controls_root"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:visibility="gone">

      <Button android:id="@+id/select_tracks_button"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:text="@string/track_selection_title"
          android:enabled="false"
          android:visibility="gone"/>

    </LinearLayout>

    <LinearLayout android:id="@+id/sonami_parent"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:orientation="vertical"
        >

      <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:app="http://schemas.android.com/apk/res-auto"
          android:layout_width="match_parent"
          android:layout_height="0dip"
          android:layout_weight="0.80"
          android:visibility="visible">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/top_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.05" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/top_mid_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.17" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/mid_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.29" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/mid_bottom_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.41" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/bottom_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.53" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/baseline_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.65" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/switch_top_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.80" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/switch_bottom_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.95" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/left_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.02" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/left_to_left_center_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.12" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/left_center_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.22" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/left_center_to_center_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.32" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/center_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.42" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/center_to_center_right_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.52" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/center_right_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.62" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/center_right_to_right_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.72" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/right_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.82" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/right_to_right_guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.92" />

        <RadioButton
            android:id="@+id/rbLChannel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:scaleX="1.25"
            android:scaleY="1.25"
            android:text="L"
            app:layout_constraintLeft_toLeftOf="@+id/left_guideline"
            app:layout_constraintTop_toTopOf="@+id/top_guideline" />

        <RadioButton
            android:id="@+id/rbCChannel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:scaleX="1.25"
            android:scaleY="1.25"
            android:text="C"
            app:layout_constraintLeft_toLeftOf="@+id/center_guideline"
            app:layout_constraintTop_toTopOf="@+id/top_guideline" />

        <RadioButton
            android:id="@+id/rbRChannel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:scaleX="1.25"
            android:scaleY="1.25"
            android:text="R"
            app:layout_constraintLeft_toLeftOf="@+id/right_guideline"
            app:layout_constraintTop_toTopOf="@+id/top_guideline" />


        <RadioButton
            android:id="@+id/rbLssChannel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:scaleX="1.25"
            android:scaleY="1.25"
            android:text="Lss"
            app:layout_constraintLeft_toLeftOf="@+id/left_guideline"
            app:layout_constraintTop_toTopOf="@+id/mid_guideline" />

        <RadioButton
            android:id="@+id/rbRssChannel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:scaleX="1.25"
            android:scaleY="1.25"
            android:text="Rss"
            app:layout_constraintLeft_toLeftOf="@+id/right_guideline"
            app:layout_constraintTop_toTopOf="@+id/mid_guideline" />


        <RadioButton
            android:id="@+id/rbLsChannel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:scaleX="1.25"
            android:scaleY="1.25"
            android:text="Ls"
            app:layout_constraintLeft_toLeftOf="@+id/left_guideline"
            app:layout_constraintTop_toTopOf="@+id/bottom_guideline" />

        <RadioButton
            android:id="@+id/rbAllChannel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="true"
            android:scaleX="1.25"
            android:scaleY="1.25"
            android:text="All"
            app:layout_constraintLeft_toLeftOf="@+id/center_guideline"
            app:layout_constraintTop_toTopOf="@+id/bottom_guideline" />

        <RadioButton
            android:id="@+id/rbRsChannel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:scaleX="1.25"
            android:scaleY="1.25"
            android:text="Rs"
            app:layout_constraintLeft_toLeftOf="@+id/right_guideline"
            app:layout_constraintTop_toTopOf="@+id/bottom_guideline" />

        <RadioButton
            android:id="@+id/rbLhChannel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:scaleX="1.25"
            android:scaleY="1.25"
            android:text="Lh"
            app:layout_constraintLeft_toLeftOf="@+id/left_center_guideline"
            app:layout_constraintTop_toTopOf="@+id/top_mid_guideline" />

        <RadioButton
            android:id="@+id/rbRhChannel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:scaleX="1.25"
            android:scaleY="1.25"
            android:text="Rh"
            app:layout_constraintLeft_toLeftOf="@+id/center_right_guideline"
            app:layout_constraintTop_toTopOf="@+id/top_mid_guideline" />

        <RadioButton
            android:id="@+id/rbLshChannel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:scaleX="1.25"
            android:scaleY="1.25"
            android:text="Lsh"
            app:layout_constraintLeft_toLeftOf="@+id/left_center_guideline"
            app:layout_constraintTop_toTopOf="@+id/mid_bottom_guideline" />

        <RadioButton
            android:id="@+id/rbRshChannel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:scaleX="1.25"
            android:scaleY="1.25"
            android:text="Rsh"
            app:layout_constraintLeft_toLeftOf="@+id/center_right_guideline"
            app:layout_constraintTop_toTopOf="@+id/mid_bottom_guideline" />

        <RadioButton
            android:id="@+id/rbTChannel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:scaleX="1.25"
            android:scaleY="1.25"
            android:text="T"
            app:layout_constraintLeft_toLeftOf="@+id/center_guideline"
            app:layout_constraintTop_toTopOf="@+id/mid_guideline" />

        <com.hear360.android.exoplayer2.demo.VerticalSeekBar
            android:id="@+id/sldEQBand0"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:max="200"
            android:min="0"
            android:progress="100"
            android:scaleX="1.4"
            android:scaleY="1.4"
            app:layout_constraintLeft_toLeftOf="@+id/left_guideline"
            app:layout_constraintTop_toTopOf="@+id/top_guideline"
            app:layout_constraintBottom_toBottomOf="@+id/bottom_guideline"
            app:layout_constraintHeight_percent="1.0" />

        <TextView
            android:id="@+id/lblEQBand0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0.0dB"
            app:layout_constraintBottom_toTopOf="@+id/baseline_guideline"
            app:layout_constraintLeft_toLeftOf="@+id/left_guideline" />

        <com.hear360.android.exoplayer2.demo.VerticalSeekBar
            android:id="@+id/sldEQBand1"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:max="200"
            android:min="0"
            android:progress="100"
            android:scaleX="1.4"
            android:scaleY="1.4"
            app:layout_constraintLeft_toLeftOf="@+id/left_to_left_center_guideline"
            app:layout_constraintTop_toTopOf="@+id/top_guideline"
            app:layout_constraintBottom_toBottomOf="@+id/bottom_guideline"
            app:layout_constraintHeight_percent="1.0"/>

        <TextView
            android:id="@+id/lblEQBand1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0.0dB"
            app:layout_constraintBottom_toTopOf="@+id/baseline_guideline"
            app:layout_constraintLeft_toLeftOf="@+id/left_to_left_center_guideline" />

        <com.hear360.android.exoplayer2.demo.VerticalSeekBar
            android:id="@+id/sldEQBand2"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:max="200"
            android:min="0"
            android:progress="100"
            android:scaleX="1.4"
            android:scaleY="1.4"
            app:layout_constraintLeft_toLeftOf="@+id/left_center_guideline"
            app:layout_constraintTop_toTopOf="@+id/top_guideline"
            app:layout_constraintBottom_toBottomOf="@+id/bottom_guideline"
            app:layout_constraintHeight_percent="1.0"/>

        <TextView
            android:id="@+id/lblEQBand2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0.0dB"
            app:layout_constraintBottom_toTopOf="@+id/baseline_guideline"
            app:layout_constraintLeft_toLeftOf="@+id/left_center_guideline" />

        <com.hear360.android.exoplayer2.demo.VerticalSeekBar
            android:id="@+id/sldEQBand3"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:max="200"
            android:min="0"
            android:progress="100"
            android:scaleX="1.4"
            android:scaleY="1.4"
            app:layout_constraintLeft_toLeftOf="@+id/left_center_to_center_guideline"
            app:layout_constraintTop_toTopOf="@+id/top_guideline"
            app:layout_constraintBottom_toBottomOf="@+id/bottom_guideline"
            app:layout_constraintHeight_percent="1.0"/>

        <TextView
            android:id="@+id/lblEQBand3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0.0dB"
            app:layout_constraintBottom_toTopOf="@+id/baseline_guideline"
            app:layout_constraintLeft_toLeftOf="@+id/left_center_to_center_guideline" />

        <com.hear360.android.exoplayer2.demo.VerticalSeekBar
            android:id="@+id/sldEQBand4"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:max="200"
            android:min="0"
            android:progress="100"
            android:scaleX="1.4"
            android:scaleY="1.4"
            app:layout_constraintLeft_toLeftOf="@+id/center_guideline"
            app:layout_constraintTop_toTopOf="@+id/top_guideline"
            app:layout_constraintBottom_toBottomOf="@+id/bottom_guideline"
            app:layout_constraintHeight_percent="1.0"/>

        <TextView
            android:id="@+id/lblEQBand4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0.0dB"
            app:layout_constraintBottom_toTopOf="@+id/baseline_guideline"
            app:layout_constraintLeft_toLeftOf="@+id/center_guideline" />

        <com.hear360.android.exoplayer2.demo.VerticalSeekBar
            android:id="@+id/sldEQBand5"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:max="200"
            android:min="0"
            android:progress="100"
            android:scaleX="1.4"
            android:scaleY="1.4"
            app:layout_constraintLeft_toLeftOf="@+id/center_to_center_right_guideline"
            app:layout_constraintTop_toTopOf="@+id/top_guideline"
            app:layout_constraintBottom_toBottomOf="@+id/bottom_guideline"
            app:layout_constraintHeight_percent="1.0"/>

        <TextView
            android:id="@+id/lblEQBand5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0.0dB"
            app:layout_constraintBottom_toTopOf="@+id/baseline_guideline"
            app:layout_constraintLeft_toLeftOf="@+id/center_to_center_right_guideline" />

        <com.hear360.android.exoplayer2.demo.VerticalSeekBar
            android:id="@+id/sldEQBand6"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:max="200"
            android:min="0"
            android:progress="100"
            android:scaleX="1.4"
            android:scaleY="1.4"
            app:layout_constraintLeft_toLeftOf="@+id/center_right_guideline"
            app:layout_constraintTop_toTopOf="@+id/top_guideline"
            app:layout_constraintBottom_toBottomOf="@+id/bottom_guideline"
            app:layout_constraintHeight_percent="1.0"/>

        <TextView
            android:id="@+id/lblEQBand6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0.0dB"
            app:layout_constraintBottom_toTopOf="@+id/baseline_guideline"
            app:layout_constraintLeft_toLeftOf="@+id/center_right_guideline" />

        <com.hear360.android.exoplayer2.demo.VerticalSeekBar
            android:id="@+id/sldEQBand7"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:max="200"
            android:min="0"
            android:progress="100"
            android:scaleX="1.4"
            android:scaleY="1.4"
            app:layout_constraintLeft_toLeftOf="@+id/center_right_to_right_guideline"
            app:layout_constraintTop_toTopOf="@+id/top_guideline"
            app:layout_constraintBottom_toBottomOf="@+id/bottom_guideline"
            app:layout_constraintHeight_percent="1.0"/>

        <TextView
            android:id="@+id/lblEQBand7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0.0dB"
            app:layout_constraintBottom_toTopOf="@+id/baseline_guideline"
            app:layout_constraintLeft_toLeftOf="@+id/center_right_to_right_guideline" />

        <com.hear360.android.exoplayer2.demo.VerticalSeekBar
            android:id="@+id/sldEQBand8"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:max="200"
            android:min="0"
            android:progress="100"
            android:scaleX="1.4"
            android:scaleY="1.4"
            app:layout_constraintLeft_toLeftOf="@+id/right_guideline"
            app:layout_constraintTop_toTopOf="@+id/top_guideline"
            app:layout_constraintBottom_toBottomOf="@+id/bottom_guideline"
            app:layout_constraintHeight_percent="1.0"/>

        <TextView
            android:id="@+id/lblEQBand8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0.0dB"
            app:layout_constraintBottom_toTopOf="@+id/baseline_guideline"
            app:layout_constraintLeft_toLeftOf="@+id/right_guideline" />

        <com.hear360.android.exoplayer2.demo.VerticalSeekBar
            android:id="@+id/sldEQBand9"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:max="200"
            android:min="0"
            android:progress="100"
            android:scaleX="1.4"
            android:scaleY="1.4"
            app:layout_constraintLeft_toLeftOf="@+id/right_to_right_guideline"
            app:layout_constraintTop_toTopOf="@+id/top_guideline"
            app:layout_constraintBottom_toBottomOf="@+id/bottom_guideline"
            app:layout_constraintHeight_percent="1.0"/>

        <TextView
            android:id="@+id/lblEQBand9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0.0dB"
            app:layout_constraintBottom_toTopOf="@+id/baseline_guideline"
            app:layout_constraintLeft_toLeftOf="@+id/right_to_right_guideline" />

        <Spinner
            android:id="@+id/lstEQPicker"
            android:gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@+id/switch_top_guideline"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            />

        <Button
            android:id="@+id/btnSaveEQ"
            android:background="#00000000"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@+id/switch_bottom_guideline"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:text="Save EQ to Preset"
          />

        <Switch
            android:id="@+id/sw_sonami"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:text="SONAMI:"
            app:layout_constraintBottom_toTopOf="@+id/switch_bottom_guideline"
            app:layout_constraintRight_toLeftOf="@+id/right_to_right_guideline"
            app:layout_constraintBottom_toBottomOf="@+id/bottom_guideline"
            app:layout_constraintHeight_percent="1.0"/>

        <Switch
            android:id="@+id/sw_channels"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:checked="false"
            android:text="Channel:"
            app:layout_constraintBottom_toTopOf="@+id/switch_bottom_guideline"
            app:layout_constraintRight_toLeftOf="@+id/left_center_guideline" />

        <Switch
            android:id="@+id/sw_eq"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checked="false"
            android:text="EQ:"
            android:visibility="gone"
            app:layout_constraintBottom_toTopOf="@+id/switch_top_guideline"
            app:layout_constraintRight_toLeftOf="@+id/left_center_guideline" />

        <androidx.constraintlayout.widget.Group
            android:id="@+id/channels_control_group"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"
            app:constraint_referenced_ids="rbLChannel, rbCChannel, rbRChannel, rbLssChannel, rbRssChannel, rbLsChannel, rbAllChannel, rbRsChannel, rbLhChannel, rbRhChannel, rbLshChannel, rbRshChannel,rbTChannel" />

        <androidx.constraintlayout.widget.Group
            android:id="@+id/eq_control_group"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"
            app:constraint_referenced_ids="sldEQBand0, sldEQBand1, sldEQBand2, sldEQBand3, sldEQBand4, sldEQBand5, sldEQBand6, sldEQBand7, sldEQBand8, sldEQBand9, lblEQBand0, lblEQBand1, lblEQBand2, lblEQBand3, lblEQBand4, lblEQBand5, lblEQBand6, lblEQBand7, lblEQBand8, lblEQBand9, lstEQPicker, btnSaveEQ" />

      </androidx.constraintlayout.widget.ConstraintLayout>


      <RelativeLayout android:id="@+id/sonami_control"
          android:layout_weight="0.20"
            android:layout_width="match_parent"
            android:layout_height="0dip">

        <View
            android:id="@+id/footer_space"
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:layout_alignParentBottom="true"
            />

        </RelativeLayout>

    </LinearLayout>

  </LinearLayout>

</FrameLayout>
